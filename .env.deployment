# ========================================
# TEL Cognitive Platform - Deployment Environment Template
# ========================================
# Copy this to .env.deployment and fill in your actual values

# ========================================
# Database Configuration
# ========================================
POSTGRES_USER=postgres
POSTGRES_PASSWORD=Target2025$
POSTGRES_DB=simstudio
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@db:5432/${POSTGRES_DB}

# ========================================
# Main Application Configuration
# ========================================
NEXT_PUBLIC_APP_URL=https://your-domain.com
BETTER_AUTH_URL=${NEXT_PUBLIC_APP_URL}
BETTER_AUTH_SECRET=your_secure_auth_secret_32_chars_min
ENCRYPTION_KEY=your_secure_encryption_key_32_chars

# ========================================
# OAuth Providers (Optional)
# ========================================
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# ========================================
# Email Service
# ========================================
RESEND_API_KEY=your_resend_api_key

# ========================================
# AI Service Providers
# ========================================
# Google AI
GOOGLE_API_KEY=your_google_ai_api_key

# Anthropic
ANTHROPIC_API_KEY=your_anthropic_api_key

# OpenAI
OPENAI_API_KEY=your_openai_api_key

# DeepSeek
DEEPSEEK_API_KEY=your_deepseek_api_key

# xAI
XAI_API_KEY=your_xai_api_key

# Mistral
MISTRAL_API_KEY=your_mistral_api_key

# Azure OpenAI
AZURE_OPENAI_API_KEY=your_azure_openai_api_key

# OpenRouter
OPENROUTER_API_KEY=your_openrouter_api_key

# ========================================
# Search Providers for Deep Research
# ========================================
# Tavily (Recommended)
TAVILY_API_KEY=your_tavily_api_key

# Firecrawl
FIRECRAWL_API_KEY=your_firecrawl_api_key

# Exa
EXA_API_KEY=your_exa_api_key

# SearXNG (Self-hosted)
SEARXNG_API_URL=https://your-searxng-instance.com

# Bocha
BOCHA_API_KEY=your_bocha_api_key

# ========================================
# Third-party Services
# ========================================
FREESTYLE_API_KEY=your_freestyle_api_key

# ========================================
# Development/Debug
# ========================================
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1
VERCEL_TELEMETRY_DISABLED=1
DOCKER_BUILD=1